<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="run_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
    <item name="android:button">@drawable/momitor_btn_bacground_selector</item>
    </style>
    <!--dialog-->
    <style name="DialogStyle" parent="@android:style/Theme.Dialog">
    <item name="android:windowBackground">@android:color/transparent</item>


    </style>
        <style name="myanimation">
        <item name="android:windowEnterAnimation">@anim/window_pop_anim</item>
        <item name="android:windowExitAnimation">@anim/popup_window_exit</item>
    </style>
</resources>
